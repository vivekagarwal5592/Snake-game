<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vivz.snakesnake">

    <application
        android:name=".Snake_Application"
        android:allowBackup="true"
        android:icon="@drawable/icon_store"
        android:label="Snake Snake"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Intro"
            android:label="Snake Snake"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Start"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Page_2"
            android:label="@string/title_activity_page_2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Level"
            android:label="@string/title_activity_level"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".Setting_Activity"
            android:label="@string/title_activity_setting_"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".About"
            android:label="@string/title_activity_about"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
